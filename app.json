{
  "expo": {
    "name": "Teen Panna",
    "slug": "teen-panna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo1.png",
    "scheme": "teenpanna",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sahilmk.teenpanna"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo1.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c58301c3-dbf5-4198-8909-0eaa9dac783e"
      }
    }
  }
}
